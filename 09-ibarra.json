{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Ibarra",
	"category": "professional",
	"keywords": [
		"serif",
		"traditional",
		"formal",
		"business",
		"professional",
		"elegant",
		"literary",
		"academic",
		"classic",
		"timeless",
		"refined",
		"artistic",
		"stylish"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Ibarra Real Nova",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/ibarra-real-nova/IbarraRealNova-VariableFont_wght.ttf"
							]
						}
					],
					"fontFamily": "\"Ibarra Real Nova\", serif",
					"name": "Ibarra Real Nova",
					"slug": "ibarra-real-nova"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/inter/InterVariable.ttf"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/inter/InterVariable-Italic.ttf"
							]
						}
					],
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "19px",
					"slug": "small",
					"fluid": {
						"min": "17px",
						"max": "19px"
					}
				},
				{
					"name": "Medium",
					"size": "22px",
					"slug": "medium",
					"fluid": {
						"min": "20px",
						"max": "22px"
					}
				},
				{
					"name": "Large",
					"size": "34px",
					"slug": "large",
					"fluid": {
						"min": "24px",
						"max": "34px"
					}
				},
				{
					"name": "Extra Large",
					"size": "52px",
					"slug": "x-large",
					"fluid": {
						"min": "36px",
						"max": "52px"
					}
				},
				{
					"name": "2X Large",
					"size": "72px",
					"slug": "xx-large",
					"fluid": {
						"min": "40px",
						"max": "72px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "600",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "12px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
					"fontWeight": "100",
					"lineHeight": "1"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "0.9"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.75"
		}
	}
}

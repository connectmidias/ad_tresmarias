{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Fjalla & Roboto",
	"category": "bold",
	"keywords": [
		"sans-serif",
		"bold",
		"display",
		"uppercase",
		"strong",
		"intense",
		"modern",
		"impactful",
		"urban",
		"powerful"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/roboto/Roboto-Light.ttf"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/roboto/Roboto-LightItalic.ttf"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/roboto/Roboto-Regular.ttf"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/roboto/Roboto-Regular.ttf"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/roboto/Roboto-Bold.ttf"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/roboto/Roboto-BoldItalic.ttf"
							]
						}
					],
					"fontFamily": "\"Roboto\", sans-serif",
					"name": "Roboto",
					"slug": "roboto"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Fjalla One",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/fjalla-one/FjallaOne-Regular.ttf"
							]
						}
					],
					"fontFamily": "\"Fjalla One\", sans-serif",
					"name": "Fjalla One",
					"slug": "fjalla-one"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "17px",
					"slug": "small",
					"fluid": {
						"min": "16px",
						"max": "17px"
					}
				},
				{
					"name": "Medium",
					"size": "22px",
					"slug": "medium",
					"fluid": {
						"min": "20px",
						"max": "22px"
					}
				},
				{
					"name": "Large",
					"size": "36px",
					"slug": "large",
					"fluid": {
						"min": "28px",
						"max": "36px"
					}
				},
				{
					"name": "Extra Large",
					"size": "56px",
					"slug": "x-large",
					"fluid": {
						"min": "42px",
						"max": "56px"
					}
				},
				{
					"name": "2X Large",
					"size": "72px",
					"slug": "xx-large",
					"fluid": {
						"min": "48px",
						"max": "72px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "500",
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"lineHeight": "1.9",
					"letterSpacing": "0.05em"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "30px",
					"letterSpacing": "0.05em"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "14px",
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "400",
					"textTransform": "uppercase",
					"letterSpacing": "0.05em"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "500",
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"lineHeight": "1.6",
					"letterSpacing": "0.05em"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "600",
					"textTransform": "uppercase"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontWeight": "400",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.6"
		}
	}
}
